{
  "expo": {
    "name": "radha-counter",
    "slug": "radha-counter",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.veer7.radhacounter",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "pwa": {
        "name": "Radha Counter",
        "themeColor": "#ffffff",
        "backgroundColor": "#ffffff",
        "display": "standalone",
        "orientation": "portrait"
      }
    },
    "extra": {
      "eas": {
        "projectId": "025aae0e-77fb-4c57-a60e-c1048d74a0fa"
      }
    }
  }
}
